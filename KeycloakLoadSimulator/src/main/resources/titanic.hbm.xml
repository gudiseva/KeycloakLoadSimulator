<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.kodiak.models.Titanic" table="jmeter_titanic">  
         
    <id name="pId" type="java.lang.Integer" column="P_ID">  
     <generator class="assigned"></generator>  
    </id>  
 
    <property name="name" type="string">
        <column name="NAME"/>
    </property>
    <property name="pClass" type="string">
        <column name="P_CLASS"/>
    </property>
    <property name="age" type="java.lang.Double">
        <column name="AGE"/>
    </property>
    <property name="sex" type="string">
        <column name="SEX"/>
    </property>
    <property name="survived" type="java.lang.Integer">
        <column name="SURVIVED"/>
    </property>
    <property name="sexcode" type="java.lang.Integer">
        <column name="SEX_CODE"/>
    </property>	

  </class>  
            
 </hibernate-mapping>